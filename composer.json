{
    "name": "helloworlddevs/pantheon-ci-tools",
    "description": "Pantheon CI/CD pipeline configuration",
    "type": "composer-plugin",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "composer-plugin-api": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "composer/composer": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "HelloWorldDevs\\PantheonCI\\": "src/"
        }
    },
    "extra": {
        "class": "HelloWorldDevs\\PantheonCI\\Plugin",
        "plugin-modifies-install-path": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
